<h1>Educations#new</h1>
<p>Find me in app/views/educations/new.html.erb</p>
<%= form_for @education do |f| %>
    <h2> Education </h2>
    <%= f.label :degree %>
    <%= f.select :degree, Education.degrees %><br>
    <%= f.label :institution_name %>
    <%= f.text_field :institution_name %><br>
    <%= f.label :start_date %>
    <%= f.date_select :start_date  %><br>
    <%= f.label :end_date %>
    <%= f.date_select :end_date %><br>
    <%= f.submit "Create new education" %>
  <% end %><br>
